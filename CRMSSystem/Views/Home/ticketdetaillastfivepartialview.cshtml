@model List<CRMSSystem.Core.View.TicketViewModel>
<div class="col-md-12">
    <table class="table" id="TicketList" width="10%">
        <tr>
            <th colspan="3" style="text-align:center;font-size:20px;background-color:dimgray;color:white">Detail For Last Five Tickets</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <div class="col-md-10"></div>
                    <div class="col-md-2"></div>
                    <h3><strong style="color: #3d638a">@Html.DisplayFor(modelItem => item.Title)</strong><strong style="margin-left:1000px"><img src="~/image/eye.svg" onclick="ticketdetail('@item.Id')" /></strong></h3>
                    <br />
                    <div class="col-md-12">
                        <b>Status</b>&nbsp;<i class="glyphicon glyphicon-arrow-right"></i>@Html.DisplayFor(modelItem => item.Status)<i class="glyphicon glyphicon-option-vertical"></i>
                        <b>Type</b><i class="glyphicon glyphicon-arrow-right"></i>@Html.DisplayFor(modelItem => item.Type)<i class="glyphicon glyphicon-option-vertical"></i>
                        <b>Priority</b><i class="glyphicon glyphicon-arrow-right"></i>@Html.DisplayFor(modelItem => item.Priority)<i class="glyphicon glyphicon-option-vertical"></i>
                        <b>Assign To</b><i class="glyphicon glyphicon-arrow-right"></i>@Html.DisplayFor(modelItem => item.AssignTo)

                    </div>
                </td>
            </tr>
        }

    </table>
</div>



