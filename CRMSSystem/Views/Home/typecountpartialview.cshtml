@model List<CRMSSystem.Core.View.TypecountTicket>
<div class="col-md-6">
    <table class="table" id="TypeList" width="10%">
        <tr>
            <th colspan="3" style="text-align:center;font-size:20px;background-color:dimgray;color:white">Ticket Count By Ticket Type</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td class="intro">
                    @Html.DisplayFor(modelItem => item.TypeName)
                </td>
                <td>
                    <button class="btn" style="background-color: #3d638a;color:snow" onclick="typecount('@item.TypeName')">
                        @Html.DisplayFor(modelItem => item.TypeCount)
                    </button>
                </td>
            </tr>
        }
        
    </table>
</div>


