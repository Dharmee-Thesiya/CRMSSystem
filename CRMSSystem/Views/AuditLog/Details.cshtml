@model CRMSSystem.Core.View.AuditLogViewModel

<!DOCTYPE html>
<html>
<head>
    <style>
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }

        td, th {
            border: 1px solid #dddddd;
            text-align: left;
        }

        tr:nth-child(even) {
            background-color: #dddddd;
        }
    </style>
</head>
<body>
    <div class="col-sm-10">
        <h3 style="margin-top:80px">ActivityLog Details</h3>

        <table>
            <tr>
                <th colspan="3" style="text-align:center;font-size:20px;background-color:dimgray;color:white">AuditLog Details</th>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.UserName)</td>
                <td>@Html.DisplayFor(model => model.UserName)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.Url)</td>
                <td> @Html.DisplayFor(model => model.Url)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.ExecutionTime)</td>
                <td> @Html.DisplayFor(model => model.ExecutionTime)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.ExecutionDuration)</td>
                <td> @Html.DisplayFor(model => model.ExecutionDuration)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.ClientAddress)</td>
                <td> @Html.DisplayFor(model => model.ClientAddress)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.BrowserInfo)</td>
                <td> @Html.DisplayFor(model => model.BrowserInfo)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.HttpMethod)</td>
                <td> @Html.DisplayFor(model => model.HttpMethod)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.HttpStatusCode)</td>
                <td> @Html.DisplayFor(model => model.HttpStatusCode)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.Comments)</td>
                <td> @Html.DisplayFor(model => model.Comments)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(model => model.Parameters)</td>
                <td> @Html.DisplayFor(model => model.Parameters)</td>
            </tr>
        </table>
    </div>
</body>
</html>

<div style="margin-top:580px">
    <button id="backToList">Previous</button>
</div>


<script>
    $(document).ready(function () {
        $('#backToList').click(function () {
            history.back();
        });
    });

</script>
